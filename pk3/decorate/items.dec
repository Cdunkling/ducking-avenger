ACTOR EnemyDropSpawner
{
    Radius 1
    Height 1
    +NOCLIP
    -SOLID
    States
    {
    Spawn:
        TNT1 A 0
        TNT1 A 0 A_JumpIf(ACS_ExecuteWithResult(594,10,0,0) == 1, "Nope")
        TNT1 A 0 A_SpawnItemEx("EnemyDropSpawner1",random(-5,5),random(-5,5),random(1,10),0,0,0,0,32,0)
        stop
    Nope:
        TNT1 A 1
        stop
    }
}


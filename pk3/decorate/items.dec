ACTOR MissileAmmo : Ammo replaces Shell
{
    Inventory.PickupMessage "Picked up a Missile."
    Inventory.Amount 1
    Inventory.MaxAmount 5
    Ammo.BackpackAmount 1
    Ammo.BackpackMaxAmount 5
    Inventory.Icon "MPKUA0"
    +NOGRAVITY
    States
    {
    Spawn:
        MPKU A 10 BRIGHT
        MPKU B 4 BRIGHT
        loop
    }
}

